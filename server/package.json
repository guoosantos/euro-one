{
  "name": "euro-one-server",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "lint": "eslint .",
    "test": "node --test",
    "migrate:storage": "node scripts/migrate-storage-to-db.js",
    "healthcheck:data": "node scripts/healthcheck-data.js",
    "prisma:generate": "prisma generate --schema ../prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ../prisma/schema.prisma",
    "prisma:migrate:resolve": "prisma migrate resolve --schema ../prisma/schema.prisma",
    "postinstall": "npm run prisma:generate && node scripts/install-playwright.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "prisma": "^6.1.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "playwright": "^1.50.1",
    "pg": "^8.13.1",
    "ws": "^8.18.0"
  }
}
